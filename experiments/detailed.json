{
    "name": "move-test-hyperparams",
    "controls":
    {
        "target": "data/peppers.png",
        "color_mode": "HSL",
        "alg": "move",
        "use_steady_state": true,
        "move_fns_per_cell": 3,
        "num_cells": 50,
        "population_size": 50,
        "num_generations": 20000,
        "target_resize": [64, 64],
        
        "activations": ["sin", "gauss", "softplus", "sigmoid", "linear"],
        
        "with_grad": true,
        "sgd_learning_rate": 0.03,
        "sgd_early_stop_delta": -0.00005,
        "prob_mutate_weight":  0.0,
        "prob_mutate_bias":  0.0,
        "weight_init_std":  2.0,
        "prob_random_restart": 0.001,
        
        "use_input_bias": true,
        "use_radial_distance":true,
        "use_fourier_features": true,
        "n_fourier_features": 16,
        "num_inputs": 20,
        "fourier_feature_scale": 3.0,

        "init_connection_probability": 0.85,
        "dense_init_connections": true,

        "hidden_nodes_at_start": 128,

        "prob_add_node": 0.25,
        "prob_add_connection": 0.25,
        "prob_disable_connection": 0.15,
        "prob_remove_node": 0.15,
        
        "sgd_steps": 120,
        "sgd_early_stop": 5,
        "num_upsamples": 0,
        "num_post_conv": 0
    },
    "conditions": [
        {
            "_default": {
                
            }
        }
        
    ]
}